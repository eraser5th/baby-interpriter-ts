{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "@typescript-eslint/recommended",
        "prefer-type-alias/recommended"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off"
    }
}
